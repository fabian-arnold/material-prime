<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<name>material_prime</name>

	<ordering>
		<after>
			<name>primefaces</name>
		</after>
	</ordering>

	<application>
		<system-event-listener>
			<system-event-listener-class>org.primefaces.material.application.PostConstructApplicationEventListener</system-event-listener-class>
			<system-event-class>javax.faces.event.PostConstructApplicationEvent</system-event-class>
			<source-class>javax.faces.application.Application</source-class>
		</system-event-listener>
	</application>
	
	<component>
		<component-type>org.primefaces.material.component.ProgressBar</component-type>
		<component-class>org.primefaces.material.component.progressbar.ProgressBar</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.ScrollFire</component-type>
		<component-class>org.primefaces.material.component.scrollfire.ScrollFire</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.BreadCrumb</component-type>
		<component-class>org.primefaces.material.component.breadcrumb.BreadCrumb</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.FixedActionButtonItem</component-type>
		<component-class>org.primefaces.material.component.fixedactionbutton.FixedActionButtonItem</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.FixedActionButton</component-type>
		<component-class>org.primefaces.material.component.fixedactionbutton.FixedActionButton</component-class>
	</component>

	<component>
		<component-type>org.primefaces.material.component.Toggle</component-type>
		<component-class>org.primefaces.material.component.toggle.Toggle</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.Icon</component-type>
		<component-class>org.primefaces.material.component.icon.Icon</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.Checkbox</component-type>
		<component-class>org.primefaces.material.component.checkbox.Checkbox</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.RadioButton</component-type>
		<component-class>org.primefaces.material.component.radiobutton.RadioButton</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.Button</component-type>
		<component-class>org.primefaces.material.component.button.Button</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.SelectOne</component-type>
		<component-class>org.primefaces.material.component.selectone.SelectOne</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.Input</component-type>
		<component-class>org.primefaces.material.component.input.Input</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.Image</component-type>
		<component-class>org.primefaces.material.component.image.Image</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.Toast</component-type>
		<component-class>org.primefaces.material.component.toast.Toast</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.DatePicker</component-type>
		<component-class>org.primefaces.material.component.datepicker.DatePicker</component-class>
	</component>
	
	<component>
		<component-type>org.primefaces.material.component.Range</component-type>
		<component-class>org.primefaces.material.component.range.Range</component-class>
	</component>

	<component>
		<component-type>org.primefaces.material.component.Chips</component-type>
		<component-class>org.primefaces.material.component.chips.Chips</component-class>
	</component>

	<component>
		<component-type>org.primefaces.material.component.Tree</component-type>
		<component-class>org.primefaces.material.component.tree.Tree</component-class>
	</component>

	<render-kit>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.ProgressBarRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.progressbar.ProgressBarRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.ScrollFireRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.scrollfire.ScrollFireRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.BreadCrumbRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.breadcrumb.BreadCrumbRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.FixedActionButtonRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.fixedactionbutton.FixedActionButtonRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.ToggleRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.toggle.ToggleRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.IconRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.icon.IconRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.CheckboxRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.checkbox.CheckboxRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>		
			<renderer-type>org.primefaces.material.component.RadioButtonRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.radiobutton.RadioButtonRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.ButtonRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.button.ButtonRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.SelectOneRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.selectone.SelectOneRenderer</renderer-class>			
		</renderer>		
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.InputRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.input.InputRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.ImageRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.image.ImageRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.ToastRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.toast.ToastRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.DatePickerRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.datepicker.DatePickerRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.RangeRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.range.RangeRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.ChipsRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.chips.ChipsRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>org.primefaces.material.component</component-family>
			<renderer-type>org.primefaces.material.component.TreeRenderer</renderer-type>
			<renderer-class>org.primefaces.material.component.tree.TreeRenderer</renderer-class>
		</renderer>
	</render-kit>

</faces-config>
